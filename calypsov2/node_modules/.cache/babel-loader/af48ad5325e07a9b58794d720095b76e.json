rmissions\"\n                              label=\"Select Permissions\"\n                              multiple\n                            >\n                              <template v-slot:prepend-item>\n                                <v-list-tile\n                                  ripple\n                                  @click=\"toggleEditUserSelectAllPermissions\"\n                                >\n                                  <v-list-tile-action>\n                                    <v-icon\n                                      aria-hidden=\"false\"\n                                      :color=\"\n                                        editedUser.permissions.length > 0\n                                          ? 'indigo darken-4'\n                                          : ''\n                                      \"\n                                      >{{ icon }}</v-icon\n                                    >\n                                  </v-list-tile-action>\n                                  <v-list-tile-content>\n                                    <v-list-tile-title\n                                      >Select All</v-list-tile-title\n                                    >\n                                  </v-list-tile-content>\n                                </v-list-tile>\n                                <v-divider class=\"mt-2\"></v-divider>\n                              </template>\n                            </v-select>\n                          </v-flex>\n                        </v-layout>\n                      </v-container>\n                    </v-card-text>\n\n                    <v-card-actions>\n                      <v-spacer></v-spacer>\n                      <v-btn color=\"blue darken-1\" flat @click=\"close\"\n                        >Cancel</v-btn\n                      >\n                      <v-btn color=\"primary\" dark flat @click=\"save\"\n                        >Save</v-btn\n                      >\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <v-spacer></v-spacer>\n                <v-text-field\n                  v-model=\"search\"\n                  append-icon=\"search\"\n                  label=\"Search\"\n                  single-line\n                  hide-details\n                ></v-text-field>\n              </v-layout>\n            </v-card-title>\n            <v-data-table :headers=\"headers\" :items=\"users\" :search=\"search\">\n              <template v-slot:items=\"props\">\n                <td>{{ props.item.email }}</td>\n                <td class=\"text-xs-right\">{{ props.item.name }}</td>\n                <td class=\"text-xs-right\">{{ props.item.company }}</td>\n                <td class=\"text-xs-right\">{{ props.item.projectName }}</td>\n                <td class=\"text-xs-right\">\n                  {{ props.item.permissions.join(\", \") }}\n                </td>\n                <td class=\"justify-center layout px-0\">\n                  <v-icon small class=\"mr-2\" @click=\"editUser(props.item)\"\n                    >edit</v-icon\n                  >\n                  <v-icon small @click=\"deleteUser(props.item)\">delete</v-icon>\n                </td>\n              </template>\n              <template v-slot:no-results>\n                <v-alert :value=\"true\" color=\"error\" icon=\"warning\">\n                  Your search for \"{{ search }}\" found no results.\n                </v-alert>\n              </template>\n            </v-data-table>\n          </v-card>\n        </v-layout>\n      </v-container>\n    </v-app>\n  </div>\n</template>\n\n<script>\nexport default {};\n</script>\n\n<style>\n.container {\n  padding: 2rem 0rem;\n}\n\nh4 {\n  margin: 2rem 0rem 1rem;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}